    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OBI_0002181">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OBI_0002177"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000055"/>
                        <owl:someValuesFrom>
