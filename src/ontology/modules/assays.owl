        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">EEG</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">electroencephalography</rdfs:label>
    </owl:Class>
    


