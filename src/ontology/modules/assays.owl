        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Patch_clamp</obo:IAO_0000119>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">patch clamp assay</rdfs:label>
    </owl:Class>
    


